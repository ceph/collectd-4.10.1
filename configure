  cat >>confdefs.h <<_ACEOF
#define HAVE_MODBUS_MODBUS_H 1
_ACEOF

else
  with_libmodbus="no (modbus/modbus.h not found)"
fi

done


	CPPFLAGS="$SAVE_CPPFLAGS"
fi
if test "x$with_libmodbus" = "xyes"
then
	SAVE_CPPFLAGS="$CPPFLAGS"
	SAVE_LDFLAGS="$LDFLAGS"

	CPPFLAGS="$CPPFLAGS $with_libmodbus_cflags"
	LDFLAGS="$LDFLAGS $with_libmodbus_libs"

	{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for modbus_init_tcp in -lmodbus" >&5
$as_echo_n "checking for modbus_init_tcp in -lmodbus... " >&6; }
if test "${ac_cv_lib_modbus_modbus_init_tcp+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lmodbus  $LIBS"
