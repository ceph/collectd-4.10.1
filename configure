rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_modbus_modbus_init_tcp" >&5
$as_echo "$ac_cv_lib_modbus_modbus_init_tcp" >&6; }
if test "x$ac_cv_lib_modbus_modbus_init_tcp" = x""yes; then :
  with_libmodbus="yes"
