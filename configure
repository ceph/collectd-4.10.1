		then
			with_libupsclient_config="$withval/bin/libupsclient-config"
			with_libupsclient="use_libupsclient_config"
		else
			{ $as_echo "$as_me:${as_lineno-$LINENO}: Not checking for libupsclient: Manually configured" >&5
